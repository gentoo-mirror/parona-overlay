DESCRIPTION="Nexus Mods App, a mod installer, creator and manager for all your popular games"
HOMEPAGE="
	https://www.nexusmods.com/
	https://github.com/Nexus-Mods/NexusMods.App/
"

COMMIT="753a9b123f2a4c1b0f5048e16d04f611e8dd9b30"
SRC_URI="
	https://github.com/Nexus-Mods/NexusMods.App/archive/${COMMIT}.tar.gz -> ${MY_PN}-${COMMIT}.tar.gz
	${NUGET_URIS}
"
S="${WORKDIR}/${MY_PN}-${COMMIT}"

LICENSE="GPL-3"
SLOT="0"

RDEPEND="media-libs/fontconfig"

DOTNET_PKG_PROJECTS=( "${S}/src/NexusMods.App/NexusMods.App.csproj" )

DOTNET_PKG_BUILD_EXTRA_ARGS=( -p:Version=${MY_PV} )

src_install() {
	dotnet-pkg-base_install
	dotnet-pkg-base_dolauncher "/usr/share/${P}/NexusMods.App" "${PN}-gui"

	doicon Nexus-Icon.png
	make_desktop_entry nexusmodsapp-gui NexusMods.App Nexus-Icon
}
